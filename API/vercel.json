{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ],
  "env": {
    "POSTGRES_PORT": "5432",
    "JWT_SECRET": "secret",
    "CLIENT_URL": "https://projet-fil-rouge-b3.vercel.app"
  }
}